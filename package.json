  "type": "module",
  "scripts": {
    "dev:editor": "npm-run-all --parallel dev-ui:editor dev:tauri",
    "dev:db": "npm-run-all --parallel dev-ui:db dev:tauri",
    "dev:ai": "npm-run-all --parallel dev-ui:ai dev:tauri",
    "dev:markdown": "npm-run-all --parallel dev-ui:markdown dev:tauri",
    "dev:music": "npm-run-all --parallel dev-ui:music dev:tauri",
    "dev:redis": "npm-run-all --parallel dev-ui:redis dev:tauri",
    "dev:cluster": "npm-run-all --parallel dev-cluster:* dev:tauri",
    "dev-cluster:editor": "cd ui/editor && npm run dev:cluster",
    "dev-cluster:db": "cd ui/db && npm run dev:cluster",
    "dev-cluster:ai": "cd ui/ai && npm run dev:cluster",
    "dev-cluster:markdown": "cd ui/markdown && npm run dev:cluster",
    "dev-cluster:music": "cd ui/music && npm run dev:cluster",
    "dev-cluster:redis": "cd ui/redis && npm run dev:cluster",
    "dev-ui:editor": "cd ui/editor && npm run dev",
    "dev-ui:db": "cd ui/db && npm run dev",
    "dev-ui:ai": "cd ui/ai && npm run dev",
    "dev-ui:markdown": "cd ui/markdown && npm run dev",
    "dev-ui:music": "cd ui/music && npm run dev",
    "dev-ui:redis": "cd ui/redis && npm run dev",
    "dev:tauri": "tauri dev",
    "clean": "rimraf ui/dist",
    "build:prod": "cd crates/app && tauri build",
    "build:ui:prod": "npm-run-all --parallel clean build-ui:*",
    "build-ui:editor": "cd ui/editor && pnpm build",
    "build-ui:db": "cd ui/db && pnpm build",
    "build-ui:ai": "cd ui/ai && pnpm build",
    "build-ui:markdown": "cd ui/markdown && pnpm build",
    "build-ui:music": "cd ui/music && pnpm build",
    "build-plugins:all": "npm-run-all --parallel build-plugins:*",
    "build-plugins:mysql": "cd crates/plugins/mysql && npm run build",
    "build-plugins:sqlite": "cd crates/plugins/sqlite && npm run build",
    "build-plugins:dm": "cd crates/plugins/dm && npm run build",
    "build:win-32": "tauri build --target i686-pc-windows-msvc",
    "build:win-arm64": "tauri build --target aarch64-pc-windows-msvc",
    "build:mac-aarch64": "tauri build --target aarch64-apple-darwin",
    "build:mac-x86_64": "tauri build --target x86_64-apple-darwin",
    "build:mac-universal": "tauri build --target universal-apple-darwin"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@tauri-apps/api": "2.0.0-beta.7",
    "@tauri-apps/plugin-clipboard-manager": "2.0.0-beta.2",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.2",
    "@tauri-apps/plugin-fs": "2.0.0-beta.2",
    "@tauri-apps/plugin-global-shortcut": "2.0.0-beta.2",
    "@tauri-apps/plugin-http": "2.0.0-beta.2",
    "@tauri-apps/plugin-notification": "2.0.0-beta.2",
    "@tauri-apps/plugin-os": "2.0.0-beta.2",
    "@tauri-apps/plugin-process": "2.0.0-beta.2",
    "@tauri-apps/plugin-shell": "2.0.0-beta.2",
    "@tauri-apps/plugin-sql": "2.0.0-beta.2",
    "@tauri-apps/plugin-updater": "2.0.0-beta.2",
    "ant-design-vue": "^4.2.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "rimraf": "^5.0.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sql-formatter": "^15.3.1",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vditor": "^3.10.4",
    "vue": "^3.4.27",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "@tauri-apps/plugin-mysql": "workspace:^",
    "@tauri-apps/plugin-sqlite": "workspace:^",
    "@tauri-apps/plugin-terminal": "workspace:^",
    "palm-editor": "workspace:^"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tauri-apps/cli": "2.0.0-beta.12",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "internal-ip": "^8.0.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vue-tsc": "^2.0.19"
  }
}
