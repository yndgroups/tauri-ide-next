{
  "name": "editor",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "yangdaqiong",
    "email": "yangdaqiong@126.com",
    "url": "https://github.com/yndgroups"
  },
  "scripts": {
    "dev": "vite",
    "dev:cluster": "vite --mode cluster",
    "tauri": "tauri",
    "build": "vue-tsc --noEmit && vite build",
    "build:sql": "npm run build --workspace=@tauri-apps/plugin-plam-sql",
    "build:fs": "npm run build --workspace=@tauri-apps/plugin-plam-fs",
    "build:all": "npm-run-all --parallel build:sql build:fs build:web",
    "preview": "vite preview",
    "build:win-32": "tauri build --target i686-pc-windows-msvc",
    "build:win-arm64": "tauri build --target aarch64-pc-windows-msvc",
    "build:mac-aarch64": "tauri build --target aarch64-apple-darwin",
    "build:mac-x86_64": "tauri build --target x86_64-apple-darwin",
    "build:mac-universal": "tauri build --target universal-apple-darwin"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@tauri-apps/api": "2.0.0-beta.6",
    "@tauri-apps/plugin-clipboard-manager": "2.0.0-beta.2",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.2",
    "@tauri-apps/plugin-dm": "workspace:^",
    "@tauri-apps/plugin-fs": "2.0.0-beta.2",
    "@tauri-apps/plugin-global-shortcut": "2.0.0-beta.2",
    "@tauri-apps/plugin-http": "2.0.0-beta.2",
    "@tauri-apps/plugin-notification": "2.0.0-beta.2",
    "@tauri-apps/plugin-os": "2.0.0-beta.2",
    "@tauri-apps/plugin-process": "2.0.0-beta.2",
    "@tauri-apps/plugin-shell": "2.0.0-beta.2",
    "@tauri-apps/plugin-sql": "2.0.0-beta.2",
    "@tauri-apps/plugin-sqlite": "workspace:^",
    "@tauri-apps/plugin-updater": "2.0.0-beta.2",
    "ant-design-vue": "^4.2.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "sql-formatter": "^15.3.1",
    "vditor": "^3.10.4",
    "vue": "^3.4.27",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tauri-apps/cli": "2.0.0-beta.11",
    "@tauri-apps/plugin-mysql": "workspace:^",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "internal-ip": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-tsc": "^1.8.27"
  }
}
